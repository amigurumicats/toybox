{
  "manifest_version": 2,
  "name": "atcoder color",
  "description" : "AtCoderのコンテスト成績表のテーブルにレートに応じた色をつけます",
  "version": "0.1",
  "icons": {
    "16": "icon_16.png"
  },
  "content_scripts": [
    {
      "matches": ["https://atcoder.jp/users/*/history"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ]
}
